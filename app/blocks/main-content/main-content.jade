include ../questions/questions
include ../filter/filter
include ../question-card/question-card

mixin main-content()

	//- variables
	- var filters = ["Актуальные", "Новые", "Обсуждаемые"];

	+b('main').main-content&attributes(attributes)
		+questions
			+filter(filters)(class='--questions')
			+question-card(getData('content'))
